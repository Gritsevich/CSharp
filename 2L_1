using System;

namespace _1
{
    class Program
    {
        static void Main(string[] args)
        {
            string stroka = "Я уЧусЬ в БГуиРЕ";
            string letters = "ЁЙЦУКЕНГШЩЗХЪЭЖДЛОРПАВЫФЯЧСМИТЬБЮ";

            for (int i=0;i<stroka.Length;i++)
            {
                for(int j=0; j<letters.Length;j++)
                {
                    if (stroka[i] == letters[j])
                        Console.Write(stroka[i]);
                }
            }
        }
    }
}
